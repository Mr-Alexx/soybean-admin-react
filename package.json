{
  "name": "vendure-admin",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "packageManager": "pnpm@10.4.1",
  "description": "Vendure admin, based on React18、Vite5、TypeScript、Ant Design and UnoCSS.",
  "author": {
    "name": "Qian",
    "email": "719876205@qq.com",
    "url": "https://github.com/Mr-Alexx"
  },
  "license": "MIT",
  "homepage": "https://github.com/Mr-Alexx/soybean-admin-react",
  "repository": {
    "url": "https://github.com/Mr-Alexx/soybean-admin-react.git"
  },
  "bugs": {
    "url": "https://github.com/Mr-Alexx/soybean-admin-react/issues"
  },
  "keywords": [
    "React admin",
    "react-admin-template",
    "Vite5",
    "TypeScript",
    "Ant Design",
    "antd-admin",
    "Redux",
    "React-Router V6",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.8.10",
    "@ant-design/v5-patch-for-react-19": "1.0.3",
    "@better-scroll/core": "2.5.1",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@iconify/react": "6.0.1",
    "@reduxjs/toolkit": "2.8.2",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "ahooks": "3.9.5",
    "antd": "5.27.1",
    "clsx": "2.1.1",
    "dayjs": "1.11.18",
    "echarts": "6.0.0",
    "i18next": "25.4.2",
    "keepalive-for-react": "4.0.3",
    "lodash-es": "4.17.21",
    "motion": "12.4.7",
    "nprogress": "0.2.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-i18next": "15.7.3",
    "react-redux": "9.2.0",
    "react-router-dom": "7.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@iconify/json": "2.2.309",
    "@iconify/types": "2.0.0",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybean-react/vite-plugin-react-router": "0.7.1",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@types/gradient-string": "1.1.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.13.5",
    "@types/nprogress": "0.2.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "boxen": "8.0.1",
    "consola": "3.4.0",
    "gradient-string": "3.0.0",
    "json5": "2.2.3",
    "kolorist": "1.8.0",
    "lint-staged": "15.4.3",
    "sass": "1.85.0",
    "simple-git-hooks": "2.11.1",
    "ts-node": "^10.9.2",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "unocss": "^66.5.0",
    "unplugin-auto-import": "19.1.0",
    "unplugin-icons": "22.1.0",
    "vite": "7.1.4",
    "vite-plugin-inspect": "11.3.3",
    "vite-plugin-remove-console": "2.2.0",
    "vite-plugin-svg-icons": "2.0.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
